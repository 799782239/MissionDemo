<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yanqijs.missiondemo">

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/main_title">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MissionActivity"
            android:label="@string/mission_title" />
        <activity
            android:name=".PublishActivity"
            android:label="@string/publish_title" />
        <activity
            android:name=".JudgeActivity"
            android:label="@string/title_activity_judge" />
        <activity
            android:name=".MyMissionActivity"
            android:label="@string/mymission_title" />
        <activity
            android:name=".ChatActivity"
            android:label="@string/chat_title" />
        <activity android:name=".WebActivity"></activity>
    </application>

</manifest>